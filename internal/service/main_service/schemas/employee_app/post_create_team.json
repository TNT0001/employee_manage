{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "team": {
            "type": "object",
            "additionalProperties": false,
            "required": ["team_name"],
            "properties": {
                "team_name": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 255
                },
                "country_code": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 2
                },
                "division_name": {
                    "type": ["string", "null"],
                    "minLength": 1,
                    "maxLength": 25
                },
                "kind": {
                    "type": ["string", "null"],
                    "minLength": 1,
                    "maxLength": 25
                }
            }
        }
    }
}
